{
  "name": "inlined-copy",
  "displayName": "inlined Copy",
  "description": "Copy text inline without formatting from Markdown files",
  "version": "0.0.1",
  "publisher": "gongnyaa",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other",
    "Formatters"
  ],
  "keywords": [
    "markdown",
    "copy",
    "inline",
    "formatting",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gongnyaa/inlined-copy"
  },
  "activationEvents": [
    "onCommand:inlined-copy.copyInline"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "inlined-copy.copyInline",
        "category": "Inlined Copy",
        "title": "Copy Inline"
      }
    ],
    "keybindings": [
      {
        "command": "inlined-copy.copyInline",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "dev": "pnpm run watch"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "typescript": "^4.3.2",
    "vitest": "^3.0.8",
    "vscode-test": "^1.5.2"
  }
}
